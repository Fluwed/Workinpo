INSERT INTO FLUX VALUES('Washington assure l''Unesco de sa volont\u00e9 de restaurer son financement','dimanche, 10 novembre 2013 05:33:15','L''administration am\u00e9ricaine fera le maximum pour restaurer le financement par les Etats-Unis de l''Unesco et r\u00e9cup\u00e9rer ainsi le droit de vote perdu par Washington, a d\u00e9clar\u00e9 samedi 9 novembre l''ambassadeur am\u00e9ricain aupr\u00e8s de l''agence culturelle de l''ONU.','http://www.lemonde.fr/ameriques/article/2013/11/10/washington-assure-l-unesco-de-sa-volonte-de-restaurer-son-financement_3511358_3222.html','Am\u00e9riques : Toute l''actualit\u00e9 sur Le Monde.fr.')
INSERT INTO FLUX VALUES('Westgate : un Norv\u00e9gien d''origine somalienne aurait \u00e9t\u00e9 identifi\u00e9','vendredi, 18 octobre 2013 12:52:08','Hassan Abdi Dhuhulow est soup\u00e7onn\u00e9 par la police norv\u00e9gienne d''\u00eatre l''un des membres du commando islamiste ayant attaqu\u00e9 le centre commercial Westgate \u00e0 Nairobi.','http://www.lemonde.fr/afrique/article/2013/10/18/westgate-un-norvegien-d-origine-somalienne-aurait-ete-identifie_3498201_3212.html','Afrique : Toute l''actualit\u00e9 sur Le Monde.fr.')
